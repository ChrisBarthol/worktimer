<% provide(:title, 'All Customers') %>
<h1>All Customers</h1>

<%= will_paginate %>

<ul class="users">
  <%= render @customers %>
</ul>

<%= will_paginate %>

<h2>Import Customers</h2>
<%= form_tag import_customers_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "Import" %>
<% end %>